{
  "name": "my-awesome-app",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/history": "^4.6.0",
    "cycle-restart": "^0.2.2",
    "cycle-scripts-one-fits-all": "5.2.0",
    "cyclejs-test-helpers": "^1.4.0",
    "html-looks-like": "^1.0.3",
    "husky": "^0.14.3",
    "jsverify": "^0.8.3",
    "lint-staged": "^4.2.3",
    "prettier": "^1.7.4",
    "snabbdom-to-html": "^3.3.0"
  },
  "dependencies": {
    "@cycle/dom": "^19.0.0",
    "@cycle/history": "^6.4.0",
    "@cycle/http": "^14.4.0",
    "@cycle/isolate": "^3.1.0",
    "@cycle/run": "^3.3.0",
    "@cycle/storage": "^4.1.1",
    "@cycle/time": "^0.10.1",
    "cycle-onionify": "^4.0.0",
    "cycle-storageify": "^4.0.0",
    "cyclejs-utils": "^2.0.0",
    "cyclic-router": "^5.1.3",
    "switch-path": "^1.2.0",
    "xstream": "^11.0.0"
  },
  "scripts": {
    "precommit": "lint-staged",
    "format": "prettier --tab-width 4 --single-quote --write './**/*.{js,jsx,ts,tsx}'",
    "start": "cycle-scripts start",
    "test": "cycle-scripts test",
    "build": "cycle-scripts build",
    "eject": "cycle-scripts eject",
    "clean": "cycle-scripts clean"
  },
  "lintStaged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run format",
      "git add"
    ]
  },
  "nyc": {
    "instrument": false,
    "sourceMap": false,
    "include": [
      "src/components"
    ],
    "reporter": [
      "html",
      "text-summary"
    ]
  },
  "mocha-webpack": {
    "include": [
      "src/components/**/*.{jsx,js,ts,tsx}",
      "test/**/*.test.{js,jsx,ts,tsx}"
    ]
  }
}
